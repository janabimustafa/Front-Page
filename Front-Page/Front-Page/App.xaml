<common:BootStrapper
    x:Class="Front_Page.App"
    xmlns:common="using:Template10.Common"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Front_Page">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\Custom.Dark.xaml" />
                <ResourceDictionary Source="Styles\Custom.Light.xaml" />
                <ResourceDictionary Source="Styles\Custom.HighContrast.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</common:BootStrapper>